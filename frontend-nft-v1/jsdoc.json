{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  }
}
